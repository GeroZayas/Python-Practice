# breadth first traversal
def bft(root):
    if not root:
        return
    queue = deque([root])
    while queue:
        curr = queue.popleft()
        print(curr.data, end=" ")
        if curr.children:
            for child in curr.children:
                queue.append(child)


bft(root)





