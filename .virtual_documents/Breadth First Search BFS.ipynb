def bf_traversal(root):
    """Breadth First Traversal of Tree"""
    if not root:
        return
    queue = [root]
    while queue:
        current_node = queue.pop(0)
        print(current_node.data)
        if current_node.children is not None:
            for child in current_node.children:
                queue.append(child)





