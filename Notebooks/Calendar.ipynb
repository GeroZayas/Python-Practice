    "year = 2024\n",
    "month = 4\n",
    "ndays = calendar.monthrange(year, month)[1]\n",
    "mondays = [\n",
    "    day\n",
    "    for day in range(1, ndays + 1)\n",
    "    if calendar.weekday(year, month, day) == calendar.MONDAY\n",
    "]\n",
    "print(mondays)"
   ]
